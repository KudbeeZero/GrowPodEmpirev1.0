	intc 11 // 5000000000
	intc 7 // 32
	frame_dig 1 // firstByte: uint64
	-
	intc 12 // 45000000000
	*
	intc 7 // 32
	/
	+
	frame_bury 2 // reward: uint64

	// contracts/GrowPodEmpire.algo.ts:446
	// sendAssetTransfer({
	//         xferAsset: this.terpAsset.value,
	//         assetAmount: reward,
	//         assetReceiver: this.txn.sender,
	//       })
	itxn_begin
	intc 2 //  axfer
	itxn_field TypeEnum

	// contracts/GrowPodEmpire.algo.ts:447
	// xferAsset: this.terpAsset.value
	bytec 5 //  "terp_asset"
	app_global_get
	itxn_field XferAsset

	// contracts/GrowPodEmpire.algo.ts:448
	// assetAmount: reward
	frame_dig 2 // reward: uint64
	itxn_field AssetAmount

	// contracts/GrowPodEmpire.algo.ts:449
	// assetReceiver: this.txn.sender
	txn Sender
	itxn_field AssetReceiver

	// Fee field not set, defaulting to 0
	intc 0 // 0
	itxn_field Fee

	// Submit inner transaction
	itxn_submit

*if6_end:
