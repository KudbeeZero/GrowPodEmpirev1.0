//
// Check and mint $TERP for Pod 1.
// Hashes the terpene profile; if first byte < 32 (~12.5% chance), mints rare $TERP reward.
// Reward scales: 5,000 - 50,000 $TERP based on rarity score.
checkTerp:
	proto 0 0

	// Push empty bytes after the frame pointer to reserve space for local variables
	bytec 0 // 0x
	dupn 2

	// contracts/GrowPodEmpire.algo.ts:435
	// assert(this.stage(this.txn.sender).value === 6)
	txn Sender
	bytec 2 //  "stage"
	app_local_get
	intc 3 // 6
	==
	assert

	// contracts/GrowPodEmpire.algo.ts:436
	// assert(this.terpAsset.value !== AssetID.zeroIndex)
	bytec 5 //  "terp_asset"
	app_global_get
	intc 0 // 0
	!=
	assert

	// contracts/GrowPodEmpire.algo.ts:438
	// profileHash = sha256(this.terpeneProfile(this.txn.sender).value)
	txn Sender
	bytec 17 //  "terpene_profile"
	app_local_get
	sha256
	frame_bury 0 // profileHash: byte[32]

	// contracts/GrowPodEmpire.algo.ts:439
	// firstByte = getbyte(profileHash, 0)
	frame_dig 0 // profileHash: byte[32]
	intc 0 // 0
	getbyte
	frame_bury 1 // firstByte: uint64

	// *if6_condition
	// contracts/GrowPodEmpire.algo.ts:442
	// firstByte < 32
	frame_dig 1 // firstByte: uint64
	intc 7 // 32
	<
	bz *if6_end

	// *if6_consequent
	// contracts/GrowPodEmpire.algo.ts:443
	// reward: uint64 =
